{
  "name": "checkpoint",
  "private": "false",
  "version": "0.1.0",
  "description": "README.md",
  "preferGlobal": true,
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && mkdir dist && babel src --out-dir dist",
    "lint-staged": "lint-staged",
    "start": "babel-node src/index.js",
    "lint": "xo",
    "prepublishOnly": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markmur/checkpoint.git"
  },
  "keywords": [
    "xo",
    "prettier",
    "eslint",
    "precommit",
    "lint"
  ],
  "author": {
    "name": "Mark Murray",
    "email": "mark.murray.ie@gmail.com",
    "url": "https://markmurray.co"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/markmur/checkpoint/issues"
  },
  "homepage": "https://github.com/markmur/checkpoint",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "chalk": "^2.4.1",
    "eslint": "^5.0.1",
    "fs-extra": "^6.0.1",
    "global": "^4.3.2",
    "inquirer": "^6.0.0",
    "read-pkg": "^4.0.1",
    "regenerator": "^0.13.2",
    "rimraf": "^2.6.2",
    "webpack": "^4.12.1",
    "webpack-cli": "^3.0.8",
    "yarn-api": "^1.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-xo": "^0.23.0",
    "lint-staged": "^7.2.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.13.6",
    "prettier-eslint": "^8.8.2",
    "xo": "^0.21.1",
    "yarn": "^1.7.0"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": true
  },
  "xo": {
    "parser": "babel-eslint",
    "envs": [
      "browser",
      "node"
    ],
    "prettier": true,
    "space": true,
    "rules": {
      "camelcase": [
        2,
        {
          "properties": "never"
        }
      ]
    }
  },
  "lint-staged": {
    "**/*.js": [
      "prettier --write",
      "git add",
      "xo"
    ]
  },
  "precommit": {
    "run": [
      "lint-staged"
    ]
  }
}